<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <PackageId>Twaddle</PackageId>
    <PackageVersion>2.0</PackageVersion>
    <Authors>Robert Bickers</Authors>
    <Copyright>Copyright Â©2020 Robert Bickers</Copyright>
    <PackageIconUrl>https://raw.githubusercontent.com/RobertBickers/Twaddle/master/settings.png</PackageIconUrl>
    <PackageLicenseUrl>license.txt</PackageLicenseUrl>
    <Owners>Robert Bickers</Owners>
    <PackageProjectUrl>https://github.com/RobertBickers/Twaddle</PackageProjectUrl>
    <PackageReleaseNotes>Updated Package to use .NET 5</PackageReleaseNotes>
    <Summary>Quickly generate random content, including usernames, phone numbers, sentences, colours, dates, and more.</Summary>
    <PackageTags>word maker, twaddle, string generator, sentence, random</PackageTags>
    <Title>Twaddle</Title>
    <Description>Quickly generate random content, including usernames, phone numbers, sentences, colours, dates, and more.</Description>
  </PropertyGroup>

  <ItemGroup>
    <Folder Include="Configuration\" />
    <Folder Include="Sections\" />
  </ItemGroup>
  <ItemGroup>
    <None Update="license.txt">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>
</Project>
