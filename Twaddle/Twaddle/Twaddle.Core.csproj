<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <Authors>Robert Bickers</Authors>
    <Company>Code Treehouse</Company>
    <Product>Twaddle</Product>
    <Description>Package lets your create usernames, password, dates, phone numbers and random sentences.

</Description>
    <PackageTags>word maker, twaddle, string generator, password generator</PackageTags>
    <PackageReleaseNotes />
    <RootNamespace>CodeTreehouse.Twaddle</RootNamespace>
    <AssemblyName>CodeTreehouse.Twaddle</AssemblyName>
    <AssemblyVersion>1.0.0.2</AssemblyVersion>
    <FileVersion>1.0.0.2</FileVersion>
    <Version>1.0.2</Version>
    <PackageProjectUrl>https://github.com/RobertBickers/Twaddle</PackageProjectUrl>
    <RepositoryUrl>https://github.com/RobertBickers/Twaddle</RepositoryUrl>
    <PackageId>Twaddle</PackageId>
  </PropertyGroup>

</Project>
